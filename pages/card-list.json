      "bodyClassName": "panel-default",
      "syncLocation": false,
      "name": "sample",
      "footerToolbar": [
        "switch-per-page",
        "pagination"
      ],
  
      "columns": [
        {
          "name": "fullName",
          "label": "名字"
        },
        {

        {
          "name": "city",
          "label": "城市"
        },
        {
          "name": "state",
          "label": "州"
        },
        {
          "name": "postalCode",
          "label": "邮编"
        },
        {
          "name": "email",
          "label": "邮箱"
        },
        {
          "name": "phone",
          "label": "手机号"
        },
        {
          "name": "cardName",
          "label": "卡名"
        },
        {
          "name": "cardNumber",
          "label": "卡号"
        },
        {
          "name": "expireDate",
          "label": "过期时间"
        },
        {
          "name": "cvv",
          "label": "cvv"
        },     {
          "name": "hash",
          "label": "hash"
        }
      ],
      "affixHeader": true,
      "columnsTogglable": "auto",
